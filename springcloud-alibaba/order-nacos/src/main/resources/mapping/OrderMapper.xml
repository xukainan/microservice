<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.uaian.alibaba.mapper.OrderMapper">


    <insert id="insertOrder" keyProperty="id" parameterType="top.uaian.alibaba.model.Order">
        insert into seata_order(id, code, product, userId, money) values (#{id},#{code}, #{product}, #{userId}, #{money})
    </insert>

</mapper>